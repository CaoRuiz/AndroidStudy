    <item name="scrapped_view" type="id"/>
    <string name="app_name">PLASample</string>
    <string name="ptr_last_updated">Updated: %1$s</string>
    <string name="ptr_pull_to_refresh">Pull to refresh</string>
    <string name="ptr_refreshing">Refreshing</string>
    <string name="ptr_release_to_refresh">Release to refresh</string>
    <string name="xlistview_footer_hint_normal">查看更多</string>
    <string name="xlistview_footer_hint_ready">松开载入更多</string>
    <string name="xlistview_header_hint_loading">正在加载...</string>
